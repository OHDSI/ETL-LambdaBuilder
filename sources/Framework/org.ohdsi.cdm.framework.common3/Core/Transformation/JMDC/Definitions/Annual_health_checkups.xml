<QueryDefinition>
	<Query>
		SELECT
		CAST(RIGHT(Member_ID, LEN(Member_ID) - 1) AS BIGINT) AS person_id,
		CAST(CAST(month_and_year_of_health_chechups AS VARCHAR(6)) + '15' AS DATE) AS observation_date,
		--value_as_number
		BMI,
		abdominal_girth as Abdominal_girth,
		sbp as SBP,
		dbp as DBP,
		tc as TC,
		tg as TG,
		gpt as gpt,

		CASE
		WHEN urine_protein is NULL
		THEN NULL
		WHEN urine_protein = 1
		THEN '-'
		WHEN urine_protein = 2
		THEN '-+'
		WHEN urine_protein = 3
		THEN '+'
		WHEN urine_protein = 4
		THEN '++'
		WHEN urine_protein = 5
		THEN '+++'
		END AS Urine_protein,

		CASE
		WHEN urine_protein is NULL
		THEN NULL
		WHEN urine_protein = 1
		THEN 'Absent'
		WHEN urine_protein = 2
		THEN 'Equivocal'
		WHEN urine_protein = 3
		THEN '+'
		WHEN urine_protein = 4
		THEN '++'
		WHEN urine_protein = 5
		THEN '+++'
		END AS Urine_protein_unit,

		CASE
		WHEN urine_protein is NULL
		THEN NULL
		WHEN urine_protein = 1
		THEN 4132135
		WHEN urine_protein = 2
		THEN 4172976
		WHEN urine_protein = 3
		THEN 4123508
		WHEN urine_protein = 4
		THEN 4126673
		WHEN urine_protein = 5
		THEN 4125547
		END AS Urine_protein_unit_concept_id,

		hdl_c as HDL_C,
		ldl_c as LDL_C,

		G_GT as G_GT,
		Fasting_blood_sugar as Fasting_blood_sugar,
		Casual_blood_sugar as Casual_blood_sugar,
		HbA1c,
		Hematocrit as Hematocrit,
		hemoglobin as Hemoglobin,
		red_blood_cell_count as Red_blood_cell_count,
		Serum_uric_acid as Serum_uric_acid,
		Serum_creatinine as Serum_creatinine,

		--value_as_string
		CASE
		WHEN Urine_sugar is NULL
		THEN NULL
		WHEN Urine_sugar = 1
		THEN '-'
		WHEN Urine_sugar = 2
		THEN '-+'
		WHEN Urine_sugar = 3
		THEN '+'
		WHEN Urine_sugar = 4
		THEN '++'
		WHEN Urine_sugar = 5
		THEN '+++'
		END AS Urine_sugar,

		CASE
		WHEN Urine_sugar is NULL
		THEN NULL
		WHEN Urine_sugar = 1
		THEN 'Absent'
		WHEN Urine_sugar = 2
		THEN 'Equivocal'
		WHEN Urine_sugar = 3
		THEN '+'
		WHEN Urine_sugar = 4
		THEN '++'
		WHEN Urine_sugar = 5
		THEN '+++'
		END AS Urine_sugar_unit,

		CASE
		WHEN Urine_sugar is NULL
		THEN NULL
		WHEN Urine_sugar = 1
		THEN 4132135
		WHEN Urine_sugar = 2
		THEN 4172976
		WHEN Urine_sugar = 3
		THEN 4123508
		WHEN Urine_sugar = 4
		THEN 4126673
		WHEN Urine_sugar = 5
		THEN 4125547
		END AS Urine_sugar_unit_concept_id,

		CASE
		WHEN Ophthalmoscopy_K_W_classificatio IS NULL
		THEN NULL
		WHEN Ophthalmoscopy_K_W_classificatio = 1
		THEN '0'
		WHEN Ophthalmoscopy_K_W_classificatio = 2
		THEN 'I'
		WHEN Ophthalmoscopy_K_W_classificatio = 3
		THEN 'IIa'
		WHEN Ophthalmoscopy_K_W_classificatio = 4
		THEN 'IIb'
		WHEN Ophthalmoscopy_K_W_classificatio = 5
		THEN 'III'
		ELSE 'IV'
		END AS Ophthalmoscopy_K_W_classificatio,

		CASE
		WHEN Ophthalmoscopy_Scheie_Classifica IS NULL
		THEN NULL
		WHEN Ophthalmoscopy_Scheie_Classifica = 1
		THEN '0'
		WHEN Ophthalmoscopy_Scheie_Classifica = 2
		THEN '1'
		WHEN Ophthalmoscopy_Scheie_Classifica = 3
		THEN '2'
		WHEN Ophthalmoscopy_Scheie_Classifica = 3
		THEN '3'
		ELSE '4'
		END AS Ophthalmoscopy_Scheie_Classifica,

		CASE
		WHEN Ophthalmoscopy_Scheie_Classific0 IS NULL
		THEN NULL
		WHEN Ophthalmoscopy_Scheie_Classific0 = 1
		THEN '0'
		WHEN Ophthalmoscopy_Scheie_Classific0 = 2
		THEN '1'
		WHEN Ophthalmoscopy_Scheie_Classific0 = 3
		THEN '2'
		WHEN Ophthalmoscopy_Scheie_Classific0 = 3
		THEN '3'
		ELSE '4'
		END AS Ophthalmoscopy_Scheie_Classific0,

		CASE
		WHEN Ophthalmoscopy_SCOTT_classificat IS NULL
		THEN NULL
		WHEN Ophthalmoscopy_SCOTT_classificat = 1
		THEN 'I (a)'
		WHEN Ophthalmoscopy_SCOTT_classificat = 2
		THEN 'I (b)'
		WHEN Ophthalmoscopy_SCOTT_classificat = 3
		THEN 'II'
		WHEN Ophthalmoscopy_SCOTT_classificat = 4
		THEN 'III (a)'
		WHEN Ophthalmoscopy_SCOTT_classificat = 5
		THEN 'III (b)'
		WHEN Ophthalmoscopy_SCOTT_classificat = 6
		THEN 'IV'
		WHEN Ophthalmoscopy_SCOTT_classificat = 7
		THEN 'V (a)'
		WHEN Ophthalmoscopy_SCOTT_classificat = 8
		THEN 'V (b)'
		ELSE 'VI'
		END AS Ophthalmoscopy_SCOTT_classificat,

		CASE
		WHEN eating_habit_1 IS NULL
		THEN NULL
		WHEN eating_habit_1 = 1
		THEN 'fast'
		WHEN eating_habit_1 = 2
		THEN 'normal'
		ELSE 'slow'
		END AS Eating_speed,

		CASE
		WHEN eating_habit_2 IS NULL
		THEN NULL
		WHEN eating_habit_2 = 1
		THEN 'yes'
		ELSE 'no'
		END AS Eat_snack,

		CASE
		WHEN eating_habit_3 IS NULL
		THEN NULL
		WHEN eating_habit_3 = 1
		THEN 'yes'
		ELSE 'no'
		END AS night_meal,

		CASE
		WHEN eating_habit_4 IS NULL
		THEN NULL
		WHEN eating_habit_4 = 1
		THEN 'yes'
		ELSE 'no'
		END AS Skip_breakfast,

		CASE
		WHEN Drinking_habits IS NULL
		THEN NULL
		WHEN Drinking_habits = 1
		THEN 'everyday'
		WHEN Drinking_habits = 2
		THEN 'sometimes'
		ELSE 'rare'
		END AS Drinking_habits,

		Electrocardiogram as Electrocardiogram,

		CASE
		WHEN Electrocardiogram IS NULL THEN 0
		ELSE 1
		END AS Condition,

		CASE
		WHEN Electrocardiogram IS NULL
		THEN NULL
		WHEN Electrocardiogram = 1
		THEN 320536
		ELSE 4065279
		END AS ecg_concept_id,

		CASE
		WHEN Electrocardiogram IS NULL
		THEN NULL
		WHEN Electrocardiogram = 1
		THEN 'Electrocardiogram abnormal'
		ELSE 'ECG normal'
		END AS ecg_concept_name,

		CASE
		WHEN Smoking_habits IS NULL
		THEN NULL
		WHEN Smoking_habits = 1
		THEN 4188539
		ELSE 4188540
		END AS Smoking_habits_value,

		CASE
		WHEN Smoking_habits IS NULL
		THEN NULL
		WHEN Smoking_habits = 1
		THEN 4298794
		ELSE 4222303
		END AS Smoking_habit_concept_id,

		CASE
		WHEN Smoking_habits IS NULL
		THEN NULL
		WHEN Smoking_habits = 1
		THEN 'Smoker'
		ELSE 'Non-smoker'
		END AS Smoking_habit_concept_name,

		CASE
		WHEN Sleeping IS NULL
		THEN NULL
		WHEN Sleeping = 1
		THEN 4188539
		ELSE 4188540
		END AS Sleeping_value,
		40 as Abdominal_girth_min, 250 as Abdominal_girth_max,
		60 as SBP_min, 300 as SBP_max,
		30 as DBP_min, 150 as DBP_max,
		30 as TC_min, 1500 as TC_max,
		10 as TG_min, 2000 as TG_max,
		10 as HDL_C_min, 500 as HDL_C_max,
		20 as LDL_C_min, 1000 as LDL_C_max,
		1000 as GPT_max,
		1000 as G_GT_max,
		20 as Fasting_blood_sugar_min, 600 as Fasting_blood_sugar_max,
		50 as Casual_blood_sugar_min, 500 as Casual_blood_sugar_max,
		3 as HbA1c_min, 20 as HbA1c_max,
		1000 as Hematocrit_max,
		30 as Hemoglobin_max,
		1000 as Red_blood_cell_count_max,
		100 as Serum_uric_acid_max,
		100 as Serum_creatinine_max
		FROM {sc}.health_checkups h
		JOIN {ch_sc}._chunks ch ON ch.ChunkId = {0} AND h.Member_ID = ch.PERSON_SOURCE_VALUE
		order by CAST(RIGHT(Member_ID, LEN(Member_ID) - 1) AS BIGINT)
	</Query>

