<QueryDefinition>
	<Variables>
		<Variable name="providField" database="ccae, mdcr">CAST(PROVID AS VARCHAR(15))</Variable>
		<Variable name="providField" database="mdcd">PROV_ID</Variable>

		<Variable name="procmod" database="ccae, mdcr">PROCMOD</Variable>
		<Variable name="procmod" database="mdcd">NULL AS PROCMOD</Variable>

		<Variable name="diagnosisField" database="ccae, mdcr">DX1, DX2, DX3, DX4, DX5</Variable>
		<Variable name="diagnosisField" database="mdcd">DX1, DX2, DX3, DX4, null DX5</Variable>

		<Variable name="vendor" database="ccae">'ccae' as vendor</Variable>
		<Variable name="vendor" database="mdcr">'mdcr' as vendor</Variable>
		<Variable name="vendor" database="mdcd">'mdcd' as vendor</Variable>

		<Variable name="ccae_mdcr" database="ccae, mdcr">1 as ccae_mdcr</Variable>
		<Variable name="ccae_mdcr" database="mdcd">0 as ccae_mdcr</Variable>

		<Variable name="field2" database="ccae,mdcr">hlthplan</Variable>
		<Variable name="field2" database="mdcd">1 as hlthplan</Variable>
	</Variables>
	<Query>
		select {diagnosisField}, PROC1, ENROLID, PROCGRP,
		case when STDPLAC &lt; 10 then '0' || cast(STDPLAC as varchar(5)) else cast(STDPLAC as varchar(5)) end STDPLAC,
		STDPROV,
		TSVCDAT,
		SVCDATE,
		PAY as TOTAL_PAID,
		REVCODE,
		RTRIM(LTRIM(COALESCE({providField}, ''))) PROVID,
		FACHDID, YEAR,
		RTRIM(LTRIM(COALESCE(CAST(STDPROV AS VARCHAR(5)), ''))) || ';' || RTRIM(LTRIM(COALESCE({providField}, ''))) ProviderIdKey,
		SEQNUM,
		1 priority,
		44818668 CurrencyConceptId,
		{procmod},
		CASE WHEN DXVER IS NULL OR DXVER NOT IN ('0', '9') THEN CASE WHEN SVCDATE &lt; to_date('10/01/2015', 'MM/dd/yyyy') THEN '9' ELSE '0' END ELSE DXVER END as DXVER, CASE WHEN PROCTYP = '' OR PROCTYP IS NULL THEN CASE WHEN SVCDATE &lt; to_date('10/01/2015', 'MM/dd/yyyy') THEN '9' ELSE '0' END WHEN PROCTYP = '0' THEN '0' ELSE '9' END as PROCTYP,
		{vendor},
		{ccae_mdcr},
		{field2},
		qty,
		ch.PartitionId
		from {sc}.OUTPATIENT_SERVICES
		JOIN {ch_sc}._chunks ch ON ch.ChunkId = {0} AND ENROLID = ch.PERSON_ID
		ORDER BY ch.PartitionId, ENROLID
	</Query>
	<ConditionOccurrence>
		<ConditionOccurrenceDefinition>
			<Condition>{DXVER} = 9</Condition>
			<PersonId>ENROLID</PersonId>
			<StartDate>SVCDATE</StartDate>
			<ProviderIdKey>ProviderIdKey</ProviderIdKey>
			<AdditionalFields>
				<string>priority</string>
				<string>FACHDID</string>
				<string>YEAR</string>
				<string>DXVER</string>
				<string>hlthplan</string>
				<string>vendor</string>
			</AdditionalFields>
			<Concepts>
				<Concept>
					<!--<IdRequired>true</IdRequired>-->
					<ConceptIdMappers>
						<Mapper>
							<Lookup>Condition</Lookup>
						</Mapper>
					</ConceptIdMappers>
					<Fields>
						<Field key="DX1" defaultTypeId="101"/>
						<Field key="DX2" defaultTypeId="102"/>
						<Field key="DX3" defaultTypeId="103"/>
						<Field key="DX4" defaultTypeId="104"/>
						<Field key="DX5" defaultTypeId="105"/>
					</Fields>
				</Concept>
			</Concepts>
		</ConditionOccurrenceDefinition>
		<ConditionOccurrenceDefinition>
			<Condition>{DXVER} = 0</Condition>
			<PersonId>ENROLID</PersonId>
			<StartDate>SVCDATE</StartDate>
			<ProviderIdKey>ProviderIdKey</ProviderIdKey>
			<AdditionalFields>
				<string>priority</string>
				<string>FACHDID</string>
				<string>YEAR</string>
				<string>DXVER</string>
				<string>hlthplan</string>
				<string>vendor</string>
			</AdditionalFields>
			<Concepts>
				<Concept>
					<!--<IdRequired>true</IdRequired>-->
					<ConceptIdMappers>
						<Mapper>
							<Lookup>Condition_icd10</Lookup>
						</Mapper>
					</ConceptIdMappers>
					<Fields>
						<Field key="DX1" defaultTypeId="101"/>
						<Field key="DX2" defaultTypeId="102"/>
						<Field key="DX3" defaultTypeId="103"/>
						<Field key="DX4" defaultTypeId="104"/>
						<Field key="DX5" defaultTypeId="105"/>
					</Fields>
				</Concept>
			</Concepts>
		</ConditionOccurrenceDefinition>
	</ConditionOccurrence>
	<DrugExposure>
		<DrugExposureDefinition>
			<PersonId>ENROLID</PersonId>
			<StartDate>SVCDATE</StartDate>
			<EndDate>SVCDATE</EndDate>
			<AdditionalFields>
				<string>vendor</string>
				<string>hlthplan</string>
			</AdditionalFields>
			<Concepts>
				<Concept>
					<!--<IdRequired>true</IdRequired>-->
					<ConceptIdMappers>
						<Mapper>
							<Lookup>Drug</Lookup>
						</Mapper>
					</ConceptIdMappers>
					<Fields>
						<Field key="PROC1" defaultTypeId="1"/>
					</Fields>
				</Concept>
			</Concepts>
		</DrugExposureDefinition>
	</DrugExposure>
	<ProcedureOccurrence>
		<ProcedureOccurrenceDefinition>
			<PersonId>ENROLID</PersonId>
			<StartDate>SVCDATE</StartDate>
			<EndDate>TSVCDAT</EndDate>
			<ProviderIdKey>ProviderIdKey</ProviderIdKey>
			<Quantity>qty</Quantity>
			<AdditionalFields>
				<string>priority</string>
				<string>FACHDID</string>
				<string>YEAR</string>
				<string>DX1</string>
				<string>PROVID</string>
				<string>STDPROV</string>
				<string>PROCTYP</string>
				<string>hlthplan</string>
				<string>vendor</string>
			</AdditionalFields>
			<Concepts>
				<Concept>
					<!--<IdRequired>true</IdRequired>-->
					<ConceptIdMappers>
						<Mapper>
							<Lookup>Procedure</Lookup>
						</Mapper>
					</ConceptIdMappers>
					<Fields>
						<Field key="PROC1" defaultTypeId="1" isNullable="true"/>
					</Fields>
				</Concept>
				<Concept>
					<ConceptIdMappers>
						<Mapper>
							<Lookup>Modifier</Lookup>
						</Mapper>
					</ConceptIdMappers>
					<Fields>
						<Field key="PROCMOD"/>
					</Fields>
				</Concept>
			</Concepts>
		</ProcedureOccurrenceDefinition>
	</ProcedureOccurrence>
	<VisitOccurrence>
		<VisitOccurrenceDefinition>
			<PersonId>ENROLID</PersonId>
			<StartDate>SVCDATE</StartDate>
			<EndDate>TSVCDAT</EndDate>
			<ProviderIdKey>ProviderIdKey</ProviderIdKey>
			<AdditionalFields>
				<string>FACHDID</string>
				<string>YEAR</string>
				<string>PROVID</string>
				<string>STDPROV</string>
				<string>vendor</string>
				<string>hlthplan</string>
			</AdditionalFields>
			<Concepts>
				<Concept>
					<ConceptIdMappers>
						<Mapper>
							<Lookup>CMS</Lookup>
						</Mapper>
					</ConceptIdMappers>
					<Fields>
						<Field isNullable="true" key="STDPLAC" defaultConceptId="0" defaultTypeId="32860"/>
					</Fields>
				</Concept>
			</Concepts>
		</VisitOccurrenceDefinition>
	</VisitOccurrence>
	<VisitCost>
		<VisitCostDefinition>
			<TotalPaid>TOTAL_PAID</TotalPaid>
			<CurrencyConceptId>CurrencyConceptId</CurrencyConceptId>
		</VisitCostDefinition>
	</VisitCost>
	<Observation>
		<ObservationDefinition>
			<PersonId>ENROLID</PersonId>
			<StartDate>SVCDATE</StartDate>
			<EndDate>TSVCDAT</EndDate>
			<ProviderIdKey>ProviderIdKey</ProviderIdKey>
			<Concepts>
				<Concept>
					<ConceptIdMappers>
						<Mapper>
							<Lookup>Revenue</Lookup>
						</Mapper>
					</ConceptIdMappers>
					<Fields>
						<Field key="REVCODE" defaultTypeId="32860"/>
					</Fields>
				</Concept>
			</Concepts>
		</ObservationDefinition>
	</Observation>
</QueryDefinition>
