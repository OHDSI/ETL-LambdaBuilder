      </AdditionalFields>
      <Concepts>
        <Concept>
          <IdRequired>true</IdRequired>
          <ConceptIdMappers>
            <Mapper>
              <Lookup>Procedure</Lookup>
            </Mapper>
          </ConceptIdMappers>
          <Fields>
